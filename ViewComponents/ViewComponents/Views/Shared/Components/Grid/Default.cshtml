@model PersonGridModels
<div class="box">
    <h3>@Model.GridName</h3>
    <table class="table w-100">
        <thead>
            <tr>
                <th>sl. no</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Person person in Model.Persons){
                <tr>
                    <td>@person.PersonName</td>
                    <td>@person.JobTitle</td>
                </tr>
            }
        </tbody>
    </table>
</div>